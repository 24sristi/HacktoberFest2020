#include<stdio.h>
#include<math.h> /* has  sin(), abs(), and fabs() */
int main(void)
{ 
double interval;
int i;
for(i = 0; i <30; i++)
{
 interval = i/10.0;
 printf("sin( %lf ) = %lf \t", interval, fabs(sin(interval)));   //for sine function
}
printf("\n\n");
for(i=0;i<30;i++)
{
    interval=i/10.0;
    printf("cos( %lf )= %lf \t",interval,fabs(cos(interval)));   //for cos function
}
return 0;
}
